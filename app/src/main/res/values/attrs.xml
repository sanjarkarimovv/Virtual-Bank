<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextField">
        <attr name="inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="password" value="2" />
            <enum name="datetime" value="3" />
            <enum name="none" value="4" />
        </attr>
        <attr name="helperText" format="string" />
        <attr name="endIcon" format="reference" />
        <attr name="hintText" format="string" />
        <attr name="focusable" format="boolean" />
        <attr name="startIcon" format="reference" />
        <attr name="boxBackgroundColor" format="color" />
        <attr name="hintTextColor" format="color" />
        <attr name="labelColor" format="color" />
        <attr name="errorText" format="string" />
        <attr name="clearable" format="boolean" />
        <attr name="boxStrokeColor" format="color" />
        <attr name="defaultStrokeColor" format="color" />
        <attr name="maxLength" format="integer" />
        <attr name="boxStrokeRadius" format="dimension" />
        <attr name="scrollHorizontally" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomProfileDataTextView">
        <attr name="etTitle" format="string" />
        <attr name="etText" format="string" />
        <attr name="etPutTitleCenterHorizontally" format="integer" />
        <attr name="etTitleSize" format="dimension" />
        <attr name="etBoxStrokeBottomStartRadius" format="dimension" />
        <attr name="etBoxStrokeBottomEndRadius" format="dimension" />
        <attr name="etBoxStrokeTopStartRadius" format="dimension" />
        <attr name="etBoxStrokeTopEndRadius" format="dimension" />
        <attr name="etEtBoxStrokeRadius" format="dimension" />
        <attr name="etTextSize" format="dimension" />
        <attr name="etMarginBottomOfTitle" format="dimension" />
        <attr name="etBoxStrokeColor" format="color" />
        <attr name="etFocusedTitleColor" format="color" />
        <attr name="etNoneFocusedTitleColor" format="color" />
        <attr name="etTextColor" format="color" />
        <attr name="etBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="LoadingButton">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <!--        <attr name="cornerRadius" format="integer" />-->
        <attr name="progressBarTintColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CustomEnterCodeEditText">
        <attr name="exampleAttribute" format="string" />
        <attr name="setText" format="string" />
        <attr name="verifyBoxBackgroundColor" format="color" />
        <attr name="verifyBoxStrokeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="VirtualBankProgressButton">
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="VirtualBankInputSimple">
        <attr name="hintText" />
        <attr name="helperText" />
        <attr name="errorEnabled" />
    </declare-styleable>

    <declare-styleable name="VirtualBankToolbar">
        <attr name="toolbarColor" format="color" />
        <attr name="title" format="string" />
        <attr name="toolbarEndIcon" format="reference" />
        <attr name="navigationIcon" format="reference" />
    </declare-styleable>


</resources>